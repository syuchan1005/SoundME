{{#if list.length}}
    <script src="/js/category.js"></script>
    <div id="category">
        <div id="category-list" class="scrollable" data-type="{{page}}">
            {{#each list}}
                <div class="category-item">
                    <img class="album" width="45" height="45" src="/no_art.png">
                    <div class="ellipsis">{{this}}</div>
                </div>
            {{/each}}
        </div>
        <div id="category-album" class="scrollable">
        </div>
    </div>
{{else}}
    <div style="text-align: center; font-size: 20px; margin-top: 40vh">
        曲がありません
    </div>
{{/if}}